! Use extended color scheme
rofi.color-enabled:                  true
! Color scheme for normal row
rofi.color-window: argb:c5fcfcfc, argb:0002b36, argb:0000000
rofi.color-normal: argb:0fcfcfc, #474747, argb:0fcfcfc, #ffe800, #fcfcfc
rofi.color-active: #fcfcfc, #ffe800, #fcfcfc, #ffe800, #fcfcfc
rofi.color-urgent: argb:0fcfcfc, #dc322f, argb:0fcfcfc, #dc322f, #fdf6e3
! Separator style (none, dash, solid)
rofi.separator-style:                none

! special
*.foreground:   #c5c8c6
*.background:   #1d1f21
*.cursorColor:  #c5c8c6

! black
*.color0:       #34363a
*.color8:       #494e56

! red
*.color1:       #a54242
*.color9:       #cc6666

! green
*.color2:       #8c9440
*.color10:      #b5bd68

! yellow
*.color3:       #de935f
*.color11:      #f0c674

! blue
*.color4:       #a54242
*.color12:      #cc6666

! magenta
*.color5:       #85678f
*.color13:      #b294bb

! cyan
*.color6:       #5e8d87
*.color14:      #8abeb7

! white
*.color7:       #707880
*.color15:      #c5c8c6

